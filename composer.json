{
    "name": "romeoz/rock-date",
    "description": "A simple DateTime library for PHP with I18N",
    "keywords": ["date", "datetime", "i18n"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock-date/issues",
        "source": "https://github.com/romeOz/rock-date"
    },
    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/romeOz/rock-base"
    }],
    "require": {
        "php": ">=5.4.0",
        "romeoz/rock-base" : "*@dev",
        "romeoz/rock-helpers" : "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*"
    },
    "autoload": {
        "psr-4": {
            "rock\\date\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}